<?xml version="1.0" encoding="UTF-8"?>
<android.widget.ContactCardView
    android:gravity="center_horizontal"
    android:id="@id/favorites_contacts_card"
    android:tag="tag_favorites_contacts_card_view"
    android:layout_width="fill_parent"
    android:layout_height="378.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:gravity="center_horizontal"
        android:id="@id/favorites_contacts_card"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <ImageView
            android:background="@drawable/contact_photo_bkg"
            android:layout_width="353.29688dip"
            android:layout_height="370.59375dip"
            android:layout_marginTop="1.2999878dip"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:id="@id/favorites_contacts_card_photo"
            android:tag="tag_favorites_contacts_card_photo"
            android:layout_width="342.6953dip"
            android:layout_height="326.6953dip"
            android:layout_marginLeft="8.0dip"
            android:layout_marginTop="7.0dip"
            android:src="@drawable/contact_photo_default_big"
            android:scaleType="fitXY"
            />
        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:src="@drawable/background_card"
            android:scaleType="fitXY"
            />
    </RelativeLayout>
    <LinearLayout
        android:gravity="center_horizontal"
        android:id="@id/favorites_contacts_card_call"
        android:tag="tag_favorites_contacts_card_call"
        android:background="@drawable/background_call"
        android:layout_width="342.6953dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="262.39844dip"
        android:layout_centerHorizontal="true">
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <ImageView
                android:id="@id/favorites_contacts_card_email"
                android:tag="tag_favorites_contacts_card_email"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/selector_icon_email"
                />
            <ImageView
                android:id="@id/favorites_contacts_card_email_disable"
                android:tag="tag_favorites_contacts_card_email_disable"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_email_pressed"
                />
            <ImageView
                android:id="@id/favorites_contacts_card_phone"
                android:tag="tag_favorites_contacts_card_phone"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="59.299988dip"
                android:src="@drawable/selector_icon_call"
                />
            <ImageView
                android:id="@id/favorites_contacts_card_phone_disable"
                android:tag="tag_favorites_contacts_card_phone_disable"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="59.299988dip"
                android:src="@drawable/icon_call_pressed"
                />
            <ImageView
                android:id="@id/favorites_contacts_card_mms"
                android:tag="tag_favorites_contacts_card_mms"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="59.299988dip"
                android:src="@drawable/selector_icon_mms"
                />
            <ImageView
                android:id="@id/favorites_contacts_card_mms_disable"
                android:tag="tag_favorites_contacts_card_mms_disable"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="59.299988dip"
                android:src="@drawable/icon_mms_pressed"
                />
        </LinearLayout>
    </LinearLayout>
    <RelativeLayout
        android:id="@id/favorites_contacts_card_sim"
        android:tag="tag_favorites_contacts_card_sim"
        android:visibility="invisible"
        android:layout_width="153.0dip"
        android:layout_height="62.0dip"
        android:layout_marginTop="172.0dip"
        android:layout_centerHorizontal="true">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/background_sim"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:tag="tag_favorites_contacts_card_sim1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22.0dip"
            android:layout_marginTop="7.0dip"
            android:src="@drawable/selector_icon_sim1"
            />
        <ImageView
            android:tag="tag_favorites_contacts_card_sim1_disable"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22.0dip"
            android:layout_marginTop="7.0dip"
            android:src="@drawable/call_by_sim1_disable"
            />
        <ImageView
            android:tag="tag_favorites_contacts_card_sim2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="98.0dip"
            android:layout_marginTop="7.0dip"
            android:src="@drawable/selector_icon_sim2"
            />
        <ImageView
            android:tag="tag_favorites_contacts_card_sim2_disable"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="98.0dip"
            android:layout_marginTop="7.0dip"
            android:src="@drawable/call_by_sim2_disable"
            />
    </RelativeLayout>
    <ImageView
        android:id="@id/favorites_contacts_card_back"
        android:tag="tag_favorites_contacts_card_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:layout_marginRight="5.0dip"
        android:src="@drawable/selector_icon_back"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        />
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11.0dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true">
        <TextView
            android:textSize="21.0sp"
            android:typeface="sans"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@id/favorites_contacts_card_name"
            android:tag="tag_favorites_contacts_card_name"
            android:layout_width="221.0dip"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:layout_centerHorizontal="true"
            />
    </RelativeLayout>
</android.widget.ContactCardView>
